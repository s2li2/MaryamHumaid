class SARA{
	int lengtTTh, breadtTTTh;
	
	SARA()
	{ length= 19;
	  breadth= 50;
	  
	}
	SARA(int L,int B)
	{
	lengtTTh= L;
	breadtTTTh= B;
	
	}
	void check()
  {
	if (lengtTTh== breadtTTTh){
		System.out.println("It is square");
		}
		
	else{
		System.out.println("It is rectangle");
				
         }
		
  }
}

